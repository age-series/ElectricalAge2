{

    "multipart": [
        {
            "when": {
                "connect_east": "true"
            },
            "apply": {
                "model": "eln2:block/wire",
                "y": 90

            }
        },
        {
            "when": {
                "connect_north": "true"
            },
            "apply": {
                "model": "eln2:block/wire",
                "y": 0

            }
        },
        {
            "when": {
                "connect_south": "true"
            },
            "apply": {
                "model": "eln2:block/wire",
                "y": 180

            }
        },
        {
            "when": {
                "connect_west": "true"
            },
            "apply": {
                "model": "eln2:block/wire",
                "y": 270

            }
        }
    ]

}
